<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns     = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"

        id        = "org.uwap.feed"
        version   = "0.1.0">
    <name>UWAP Feed</name>

    <description>
        An aggregated federated newsfeed of relevant activities in collaboration services for lower and higher education in Norway.
    </description>

    <author href="http://rnd.feide.no" email="andreas.solberg@uninett.no">
        Andreas Ã…kre Solberg
    </author>

    <feature name="http://api.phonegap.com/1.0/device" />

    <preference name="phonegap-version" value="2.2.0" />
    <preference name="orientation"      value="default" />
    <preference name="target-device"    value="universal" />
    <preference name="fullscreen"       value="false" />

